{
  "name": "code-solicitar",
  "version": "0.1.0",
  "description": "Sistema para Controle de Requisições e Protocolo",
  "main": "index.js",
  "repository": "https://github.com/codemastersolutions/cms-requisicoes",
  "author": "Gilson Gabriel <gilson@codemastersolucoes.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/framework/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/framework/server.ts",
    "test": "jest",
    "seed:config": "ts-node-dev ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node-dev ./node_modules/typeorm-seeding/dist/cli.js seed",
    "orm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "db:fresh": "yarn orm migration:revert && yarn orm migration:run",
    "db:fresh:seed": "yarn orm migration:revert && yarn orm migration:run && yarn seed:run",
    "db:run": "yarn orm migration:run",
    "db:show": "yarn orm migration:show"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.21.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^5.4.0",
    "graphql": "^15.5.0",
    "mssql": "^6.3.1",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1"
  }
}